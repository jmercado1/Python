invest_number = int(input("investigative number: "))
while invest_number >= 10 :
    print ("Enter number from 0 to 10")
    invest_number = int(input("investigative number: "))

while invest_number <= -1:
    print ("Enter number from 0 to 10")
    invest_number = int(input("investigative number: "))

low_range_number = int(input("low range number: "))
while low_range_number <= -1:
    print ("Choose high number")
    low_range_number = int(input("low range number: "))

high_range_number = int(input("high range number: "))
while high_range_number < low_range_number:
    print("Choose number higher than previous")
    high_range_number = int(input("high range number: "))

    for i in range (low_range_number, high_range_number + 1 ):
        print(invest_number, "x", i, "=") (invest_number*i)
        